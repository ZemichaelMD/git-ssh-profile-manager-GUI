{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "types.d.ts"],
    "exclude": ["node_modules"]
  }
  